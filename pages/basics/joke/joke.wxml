<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="content">最新笑话</view>
</cu-custom>
<view class="cu-card article {{isCard?'no-card':''}}">
  <!-- <image class="" src="/images/home/joke.jpg" mode="widthFix" lazy-load="false" binderror="" bindload="" /> -->
  <view class="cu-item shadow">
    <view class="text-content">
      <rich-text nodes="{{jokeData[subscript].content}}"></rich-text>
    </view>
  </view>
</view>
<view class="padding flex flex-direction btn-wrapper" bindtap="handleNext">
    <button class="cu-btn lg round bg-olive">下一个</button>
</view>  