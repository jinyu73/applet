<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="content">舔狗日记</view>
</cu-custom>
<view class="content-wrapper">
    <image class="" src="/images/home/diary.png" mode="widthFix" lazy-load="false" binderror="" bindload="" />
    <text class="content-text" selectable="false" space="false" decode="false">
      {{diaryData}}
    </text>
        
</view>
<view class="padding flex flex-direction btn-wrapper" bindtap="handleNext">
    <button class="cu-btn lg round bg-olive">再舔一下</button>
</view>   
  
